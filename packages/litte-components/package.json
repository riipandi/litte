    "scripts": {
        "build:types": "tsc --project tsconfig.json",
        "build:js": "rollup --config rollup.config.js",
        "build": "pnpm -s build:types && pnpm -s build:js",
        "dev": "rollup --watch --config rollup.config.js",
        "cleanup": "pnpm dlx del-cli dist node_modules .turbo",
        "prepublishOnly": "NODE_ENV=production rollup --config rollup.config.js",
        "publish:npm": "NODE_ENV=production pnpm publish --no-git-checks",
        "publish:npm-dry": "NODE_ENV=production pnpm publish --no-git-checks --dry-run",
        "publish:jsr": "pnpm dlx jsr publish --allow-dirty",
        "publish:jsr-dry": "pnpm dlx jsr publish --allow-dirty --dry-run",
