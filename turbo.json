{
    "$schema": "https://turborepo.com/schema.json",
    "noUpdateNotifier": true,
    "concurrency": "24",
    "globalEnv": ["NODE_ENV", "TZ", "CI"],
    "ui": "stream",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [".next/**", "!.next/cache/**", "dist/**", "out/**"]
        },
        "lint": {},
        "check": {},
        "typecheck": {
            "dependsOn": ["^build"]
        },
        "dev": {
            "dependsOn": ["^build"],
            "persistent": true,
            "cache": false
        },
        "start": {
            "cache": false,
            "dependsOn": ["^build"],
            "persistent": true
        },
        "cleanup": {
            "cache": false
        },
        "prepublishOnly": {
            "cache": true
        },
        "publish:npm-dry": {
            "dependsOn": ["^build"],
            "cache": true
        },
        "publish:npm": {
            "dependsOn": ["^build"],
            "cache": true
        },
        "publish:jsr-dry": {
            "dependsOn": ["^build"],
            "cache": true
        },
        "publish:jsr": {
            "dependsOn": ["^build"],
            "cache": true
        },
        "litte": {
            "cache": false,
            "dependsOn": ["^build"],
            "outputLogs": "full",
            "persistent": true
        }
    }
}
