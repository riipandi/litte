{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "stream",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": [".next/**", "!.next/cache/**", "dist/**", "out/**"]
        },
        "lint": {},
        "check": {},
        "typecheck": {},
        "dev": {
            "cache": false,
            "persistent": true
        },
        "start": {
            "cache": false,
            "dependsOn": ["^build"],
            "persistent": true
        },
        "publish:dry": {},
        "publish:npm": {}
    }
}
